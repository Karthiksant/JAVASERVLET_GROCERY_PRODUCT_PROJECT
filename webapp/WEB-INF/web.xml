<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <context-param>
  	<param-name>CGST</param-name>
  	<param-value>0.15</param-value>
  </context-param>
  <servlet>
  <servlet-name>product</servlet-name>
  <servlet-class>controller.ProductSave</servlet-class>
  <init-param>
  <param-name>TS</param-name>
  <param-value>0.10</param-value>
  </init-param>
  <init-param>
  <param-name>AP</param-name>
  <param-value>0.12</param-value>
  </init-param>
  </servlet>
  <servlet-mapping>
  <servlet-name>product</servlet-name>
  <url-pattern>/save</url-pattern>
  </servlet-mapping>
</web-app>
